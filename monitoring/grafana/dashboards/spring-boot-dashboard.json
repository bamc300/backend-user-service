{
  "dashboard": {
    "id": null,
    "title": "Spring Boot Application Metrics",
    "tags": ["spring-boot", "java", "metrics"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Request Rate (req/sec)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(http_server_requests_total{job=\"spring-boot-app\"}[5m]))",
            "legendFormat": "Total Requests/sec"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 2,
        "title": "HTTP Errors (4xx/5xx)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(http_server_requests_total{job=\"spring-boot-app\",status=~\"4..|5..\"}[5m]))",
            "legendFormat": "Error Rate"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "CPU Usage (%)",
        "type": "stat",
        "targets": [
          {
            "expr": "system_cpu_usage{job=\"spring-boot-app\"} * 100",
            "legendFormat": "CPU Usage"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "max": 100,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 4,
        "title": "JVM Memory Usage (MB)",
        "type": "stat",
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{job=\"spring-boot-app\",area=\"heap\"} / 1024 / 1024",
            "legendFormat": "Heap Memory"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "decbytes",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 5,
        "title": "HTTP Requests by Endpoint",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (uri) (rate(http_server_requests_total{job=\"spring-boot-app\"}[5m]))",
            "legendFormat": "{{uri}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "yAxes": [
          {"label": "Requests/sec", "min": 0}
        ]
      },
      {
        "id": 6,
        "title": "HTTP Response Times (Percentiles)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(http_server_requests_seconds_bucket{job=\"spring-boot-app\"}[5m])) by (le))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{job=\"spring-boot-app\"}[5m])) by (le))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(http_server_requests_seconds_bucket{job=\"spring-boot-app\"}[5m])) by (le))",
            "legendFormat": "99th percentile"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "yAxes": [
          {"label": "Response Time (seconds)", "min": 0}
        ]
      },
      {
        "id": 7,
        "title": "JVM Memory Usage Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{job=\"spring-boot-app\",area=\"heap\"} / 1024 / 1024",
            "legendFormat": "Heap Used (MB)"
          },
          {
            "expr": "jvm_memory_max_bytes{job=\"spring-boot-app\",area=\"heap\"} / 1024 / 1024",
            "legendFormat": "Heap Max (MB)"
          },
          {
            "expr": "jvm_memory_used_bytes{job=\"spring-boot-app\",area=\"nonheap\"} / 1024 / 1024",
            "legendFormat": "Non-Heap Used (MB)"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "yAxes": [
          {"label": "Memory (MB)", "min": 0}
        ]
      },
      {
        "id": 8,
        "title": "HTTP Status Codes",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (status) (rate(http_server_requests_total{job=\"spring-boot-app\"}[5m]))",
            "legendFormat": "{{status}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "yAxes": [
          {"label": "Requests/sec", "min": 0}
        ]
      }
    ],
    "time": {"from": "now-1h", "to": "now"},
    "refresh": "5s"
  }
}